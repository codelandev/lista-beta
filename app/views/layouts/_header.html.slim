.menu-wrapper.navbar-fixed-top
  .container
    .logo
      = image_tag 'logo-azul.png', alt: 'Lista Beta Logo'
    = link_to root_path do
      .menu-item.menu-fade
        span STARTUPS
    .menu-button
      = link_to "ENVIE SUA STARTUP", root_path, class: 'btn default-btn fade'
    .pull-right
      = link_to root_path do
        .menu-item.last-menu-item.menu-fade
          span ENTRAR
